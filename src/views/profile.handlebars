<main class="d-flex flex-column align-items-center w-100 flex-grow-1">
  <h1 class="bg-success-subtle p-2 text-center w-100 fst-italic text-dark-emphasis"> USERS </h1>
  <section class="text-center m-3 w-100 d-flex d-flex flex-wrap justify-content-center gap-3">
    {{#each users}}
    <div class="card" style="width: 18rem;">
            <img src="{{this.foto}}" class="card-img-top" alt="{{this.id}}">
        <div class="card-body">
                <h5 class="card-title">{{this.email}}</h5>
        {{!-- <p class="card-text">Category: {{this.category}}</p>  --}}
            <div class="card-body d-flex justify-content-around">
                <strong class="card-text">Role: {{this.role}}</strong>
            </div>  
        </div>
    </div>
{{/each}}
  </section>
</main>



